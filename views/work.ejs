<%- include("partials/header.ejs") %>

<main>  <div class="box" id="heading">
    <h1>
      Work List
    </h1>
  </div>
    
    <div class="box">

      <% for(var count=0; count<newlistItems.length; count++ ){ %>
          <div class="item">
              <input type="checkbox">
              <p> <%= newlistItems[count] %> </p>
          </div>
      <%} %>

     
      
        <form class="item" action="/" method="post">
          <input type="text" name="newItem" placeholder="New Item" autocomplete="off">
          <button type="submit" name="list" value="Work">+</button>
        </form>
  </div>
  </main>

